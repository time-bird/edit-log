{
  "name": "edit-log",
  "displayName": "Edit Log",
  "publisher": "time-bird",
  "description": "Track character additions and removals per day.",
  "version": "0.2.7",
  "icon": "images/editlog_icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/time-bird/edit-log"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "wordcount",
    "character count",
    "writing",
    "history",
    "edit history"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "edit-log",
          "name": "EDIT LOG",
          "icon": "$(graph-line)",
          "visibility": "visible"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.44"
  }
}
